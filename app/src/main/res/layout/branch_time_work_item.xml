<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linear_layout"
    android:layout_width="69dp"
    android:layout_height="90dp"
    android:layout_margin="@dimen/margin_small_5"
    android:background="@drawable/layout_background_with_radius"
    android:elevation="5dp"

    android:orientation="vertical">


    <TextView
        android:id="@+id/day"
        style="@style/TextView_18sp_Roboto"
        android:layout_marginTop="@dimen/margin_small_5"
        android:layout_gravity="center"
        tools:text="Monday" />

    <TextView
        android:id="@+id/startWorkTime"
        android:layout_gravity="center"
        style="@style/TextView_18sp_Roboto"
        tools:text="19:00" />

    <TextView
        android:id="@+id/endWorkTime"
        style="@style/TextView_18sp_Roboto"
        android:layout_gravity="center"
        tools:text="22:00" />


</LinearLayout>